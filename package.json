{
  "name": "swan-monorepo",
  "version": "1.0.0",
  "description": "Swan monorepo with VitePress site and VS Code plugin",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "clean": "pnpm -r clean",
    "lint": "pnpm -r lint",
    "type-check": "pnpm -r type-check",
    "vitepress": "pnpm --filter swan",
    "vscode": "pnpm --filter swan-vscode",
    "extension": "pnpm --filter swan-vscode",
    "extension:dev": "pnpm --filter swan-vscode dev",
    "extension:compile": "pnpm --filter swan-vscode compile"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "monorepo",
    "vitepress",
    "vscode-extension"
  ],
  "author": "",
  "license": "MIT"
}
